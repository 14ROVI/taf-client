<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" fx:id="lastNewsRoot" alignment="CENTER" maxHeight="300.0" maxWidth="300.0"
      minHeight="0.0" minWidth="0.0" prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.40"
      fx:controller="com.faforever.client.hub.LastNewsController">
  <children>
    <Label fx:id="titleLabel" maxWidth="1.7976931348623157E308" styleClass="h1" text="&lt;Title&gt;"/>
    <ImageView fx:id="imageView" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true"/>
    <Label fx:id="textLabel" maxWidth="1.7976931348623157E308" text="&lt;Text&gt;" wrapText="true"/>
    <HBox alignment="CENTER_RIGHT" minHeight="0.0" minWidth="0.0">
      <children>
        <Label fx:id="authoredLabel" text="&lt;authored&gt;"/>
      </children>
      <VBox.margin>
        <Insets top="10.0"/>
      </VBox.margin>
    </HBox>
  </children>
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
  </padding>
</VBox>
