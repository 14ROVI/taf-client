<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" fx:id="lastCastRoot"
      fx:controller="com.faforever.client.hub.LastCastController" xmlns="http://javafx.com/javafx/8.0.40">
  <children>
    <Label styleClass="h1" text="&lt;Cast title&gt;"/>
    <Label text="&lt;Cast datetime&gt;"/>
    <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true"/>
    <HBox minHeight="0.0" minWidth="0.0">
      <children>
        <Label text="&lt;#likes&gt;"/>
        <Label text="&lt;#views&gt;"/>
      </children>
    </HBox>
  </children>
</VBox>
