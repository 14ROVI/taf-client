<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>


<HBox fx:id="ranked1v1Root" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.faforever.client.rankedmatch.Ranked1v1Controller">

  <children>
    <Separator focusTraversable="false" orientation="VERTICAL"/>
    <Button mnemonicParsing="false" onAction="#onAeonButtonClicked" styleClass="large-faction-button, aeon-button"/>
    <Button mnemonicParsing="false" onAction="#onUefButtonClicked" styleClass="large-faction-button, uef-button"/>
    <Button mnemonicParsing="false" onAction="#onCybranButtonClicked" styleClass="large-faction-button, cybran-button"/>
    <Button mnemonicParsing="false" onAction="#onSeraphimButtonClicked"
            styleClass="large-faction-button, seraphim-button"/>
  </children>
</HBox>
