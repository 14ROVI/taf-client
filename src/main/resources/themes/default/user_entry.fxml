<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<fx:root type="HBox" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1">
  <ImageView fx:id="avatarImageView" fitHeight="16.0" fitWidth="28.0" pickOnBounds="true" preserveRatio="true"
             HBox.hgrow="NEVER"/>
  <ImageView fx:id="countryImageView" fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true"/>
  <Label fx:id="usernameLabel" HBox.hgrow="ALWAYS">
    <padding>
      <Insets left="10.0"/>
    </padding>
  </Label>
  <ImageView fx:id="statusImageView" fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true"
             HBox.hgrow="NEVER">
    <HBox.margin>
      <Insets left="10.0"/>
    </HBox.margin>
  </ImageView>
</fx:root>
